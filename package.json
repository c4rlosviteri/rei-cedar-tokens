    "fs-extra": "^7.0.1",
    "gh-pages": "^2.0.1",
    "lodash": "^4.17.11",
    "rimraf": "^2.6.0",
    "style-dictionary": "^2.7.0"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 6.0.0"
  }
}
